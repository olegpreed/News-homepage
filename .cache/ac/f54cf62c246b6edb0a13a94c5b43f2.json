.top-menu {\r\n  flex-grow: 1;\r\n  display: flex;\r\n  justify-content: right;\r\n}\r\n\r\n.top-menu > li {\r\n  margin-left: 2.5rem;\r\n}\r\n\r\n.big-news > div:nth-child(2) {\r\n  grid-row: 2/3;\r\n  grid-column: 1/5;\r\n}\r\n\r\n.big-news h1 {\r\n  font-size: 3.6rem;\r\n  font-weight: 800;\r\n  color: $very-dark-blue;\r\n  line-height: 3.5rem;\r\n}\r\n\r\n.big-news > div:nth-child(3) {\r\n  grid-row: 2/3;\r\n  grid-column: 5/9;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: space-between;\r\n  max-width: 95%;\r\n}\r\n\r\n.big-news p {\r\n  line-height: 1.5rem;\r\n}\r\n\r\n.big-news button {\r\n  background-color: $soft-red;\r\n  color: white;\r\n  font-size: 0.8rem;\r\n  font-weight: 700;\r\n  padding: 1rem 2rem;\r\n  letter-spacing: 0.3rem;\r\n  text-transform: uppercase;\r\n}\r\n\r\n.small-news {\r\n  background-color: $very-dark-blue;\r\n  padding: 1.8rem 1.5rem;\r\n  color: $grayish-blue;\r\n}\r\n\r\n.small-news > div:nth-child(1) {\r\n  margin-bottom: 2rem;\r\n}\r\n\r\n.small-news h2 {\r\n  color: $soft-orange;\r\n  font-size: 2.5rem;\r\n  font-weight: 700;\r\n}\r\n\r\n.small-news h3 {\r\n  color: white;\r\n  font-size: 1.2rem;\r\n  font-weight: 800;\r\n}\r\n\r\n.small-news p {\r\n  display: block;\r\n  color: $grayish-blue;\r\n  margin-top: 0.7rem;\r\n  margin-bottom: 1.7rem;\r\n  border-bottom: 0.5px solid $dark-grayish-blue;\r\n  padding-bottom: 2rem;\r\n}\r\n\r\n.small-news > div:last-child > p {\r\n  margin-bottom: 0.3rem;\r\n  border-bottom: 0;\r\n  padding-bottom: 0;\r\n}\r\n\r\n.article-item {\r\n  display: flex;\r\n  gap: 1.5rem;\r\n  height: 8rem;\r\n}\r\n\r\n.article-item img {\r\n  height: 100%;\r\n  width: auto;\r\n}\r\n\r\n.article-right-section {\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: space-between;\r\n}\r\n\r\n.article-right-section > div:first-child {\r\n  color: $grayish-blue;\r\n  font-size: 2rem;\r\n  font-weight: 700;\r\n}\r\n\r\n.article-right-section h3 {\r\n  color: $very-dark-blue;\r\n  font-size: 1.15rem;\r\n  font-weight: 800;\r\n}\r\n\r\n.article-right-section > div:last-child {\r\n  max-width: 97%;\r\n}\r\n\r\n@media (width > 375px) {\r\n  .main-container {\r\n    max-width: 71rem;\r\n    margin: 0em 3em;\r\n  }\r\n  .top-header {\r\n\tmargin-top: 5.3rem;\r\n  }  \r\n  .menu-icon{\r\n\tdisplay: none;\r\n  }\r\n  .column-grid-layout {\r\n    display: grid;\r\n    grid-template-columns: repeat(12, 1fr);\r\n    grid-template-rows: auto, 300px;\r\n    gap: 4.5rem 2rem;\r\n  }\r\n  main {\r\n\tmargin-top: 3rem;\r\n\tmargin-bottom: 10rem;\r\n  }\r\n  .big-news {\r\n    grid-column: 1/9;\r\n    grid-row: 1/2;\r\n  }\r\n  .big-news {\r\n\tdisplay: grid;\r\n\tgrid-template-columns: repeat(8, 1fr);\r\n\tgap: 0rem 2rem;\r\n  }\r\n  \r\n  .big-news-image {\r\n\tgrid-row: 1/2;\r\n\tgrid-column: 1/9;\r\n  }\r\n  .big-news-image > picture > img {\r\n\tmax-width: 100%;\r\n\tmargin-bottom: 1rem;\r\n  }\r\n  .big-news > div:nth-child(2) {\r\n\tmargin-top: 0.2rem;\r\n  }\r\n  .small-news {\r\n    grid-column: 9/13;\r\n    grid-row: 1/2;\r\n  }\r\n  .articles-footer {\r\n    grid-column: 1/13;\r\n    grid-row: 2/3;\r\n  }\r\n\r\n  .articles-footer {\r\n    display: grid;\r\n    grid-template-columns: repeat(12, 1fr);\r\n    gap: 0rem 2rem;\r\n  }\r\n\r\n  .articles-footer > div:nth-child(1) {\r\n    grid-column: 1/5;\r\n  }\r\n\r\n  .articles-footer > div:nth-child(2) {\r\n    grid-column: 5/9;\r\n  }\r\n\r\n  .articles-footer > div:nth-child(3) {\r\n    grid-column: 9/13;\r\n  }\r\n}\r\n\r\n@media (width <= 375px) {\r\n\t.top-header {\r\n\t\tmargin-top: 2rem;\r\n\t  }  \r\n\t  .top-navigation {\r\n\t\tjustify-content: space-between;\r\n\t  }\r\n\t  .top-logo > img {\r\n\t\twidth: 3rem;\r\n\t  }\r\n  .top-menu {\r\n    display: none;\r\n  }\r\n  main {\r\n\tmargin-top: 2rem;\r\n\tmargin-bottom: 5rem;\r\n  }\r\n  .main-container {\r\n    margin: 0em 1em;\r\n  }\r\n  .column-grid-layout {\r\n    display: grid;\r\n\tgap: 4rem;\r\n  }\r\n  .big-news {\r\n   